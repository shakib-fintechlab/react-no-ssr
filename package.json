{
  "name": "react-no-ssr",
  "version": "2.0.0",
  "description": "React component to wrap non SSR components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-no-ssr.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run remove:dist && babel ./src --ignore ./src/__tests__ --out-dir ./dist",
    "prepublish": "npm run build",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "lint-staged",
    "remove:dist": "rimraf ./dist",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-jest-enzyme": "7.0.2",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.13.0",
    "jest": "24.8.0",
    "jest-enzyme": "7.0.2",
    "jsdom": "15.0.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rimraf": "2.6.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
